<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.rap.ui.entrypoint">
      <entrypoint
            brandingId="tideeapp.branding"
            class="org.polymap.rhei.batik.BatikApplication"
            id="tideeapp.app"
            path="/app">
      </entrypoint>
   </extension>
   <extension
         point="org.eclipse.rap.ui.branding">
      <branding
            id="tideeapp.branding"
            themeId="org.polymap.rhei.batik.materialDesign"
            title="TideeApp">
         <additionalHeaders>
            <link
                  href="batikres/css/md/flowtext.css"
                  rel="stylesheet">
            </link>
            <link
                  href="batikres/css/roboto.css"
                  rel="stylesheet">
            </link>
         </additionalHeaders>
      </branding>
   </extension>
   <extension
         point="org.polymap.rhei.batik.panels">
      <panel
            class="tideeapp.ui.StartPanel"
            id="tideeapp.StartPanel">
      </panel>
   </extension>
   <extension
         point="org.polymap.rhei.batik.design">
      <design
            class="org.polymap.rhei.batik.toolkit.md.MdAppDesign"
            id="tideeapp.design"
            servletNameMatcher="/app">
      </design>
   </extension>

</plugin>
